// The feature model 
root NAO7 {
	group [*..*] {
		Core7 group allOf {
			Behaviour7,
			Connection7
		},
		opt Motion7,
		opt Audio7,
		opt Vision7,
		Sensors7,
		opt Trackers7 
	}
	
	//Attribute declarations of the root feature
	enum env in {light , noLight }; 
	
	int speed;
	speed in [30..30];
	
	// Constraints
	Motion7 -> Sensors7;
	NAO7.env == noLight -> !NAO7.Vision7;
}
