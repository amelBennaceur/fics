// The feature model 
root NAO1 {
	group [*..*] {
		Core1 group allOf {
			Behaviour1,
			Connection1
		},
		opt Motion1,
		opt Audio1,
		opt Vision1,
		Sensors1,
		opt Trackers1 
	}
	
	//Attribute declarations of the root feature
	enum env in {light , noLight }; 
	
	int speed;
	speed in [30..30];
	
	// Constraints
	Motion1 -> Sensors1;
	NAO1.env == noLight -> !NAO1.Vision1;
}
