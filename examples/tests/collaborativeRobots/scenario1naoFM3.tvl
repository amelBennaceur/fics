// The feature model 
root NAO3 {
	group [*..*] {
		Core3 group allOf {
			Behaviour3,
			Connection3
		},
		opt Motion3,
		opt Audio3,
		opt Vision3,
		Sensors3,
		opt Trackers3 
	}
	
	//Attribute declarations of the root feature
	enum env in {light , noLight }; 
	
	int speed;
	speed in [30..30];
	
	// Constraints
	Motion3 -> Sensors3;
	NAO3.env == noLight -> !NAO3.Vision3;
}
