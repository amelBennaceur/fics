// The feature model 
root NAO9 {
	group [*..*] {
		Core9 group allOf {
			Behaviour9,
			Connection9
		},
		opt Motion9,
		opt Audio9,
		opt Vision9,
		Sensors9,
		opt Trackers9 
	}
	
	//Attribute declarations of the root feature
	enum env in {light , noLight }; 
	
	int speed;
	speed in [30..30];
	
	// Constraints
	Motion9 -> Sensors9;
	NAO9.env == noLight -> !NAO9.Vision9;
}
