// The feature model 
root NAO2 {
	group [*..*] {
		Core2 group allOf {
			Behaviour2,
			Connection2
		},
		opt Motion2,
		opt Audio2,
		opt Vision2,
		Sensors2,
		opt Trackers2 
	}
	
	//Attribute declarations of the root feature
	enum env in {light , noLight }; 
	
	int speed;
	speed in [30..30];
	
	// Constraints
	Motion2 -> Sensors2;
	NAO2.env == noLight -> !NAO2.Vision2;
}
