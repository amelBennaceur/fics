// The feature model 
root NAO8 {
	group [*..*] {
		Core8 group allOf {
			Behaviour8,
			Connection8
		},
		opt Motion8,
		opt Audio8,
		opt Vision8,
		Sensors8,
		opt Trackers8 
	}
	
	//Attribute declarations of the root feature
	enum env in {light , noLight }; 
	
	int speed;
	speed in [30..30];
	
	// Constraints
	Motion8 -> Sensors8;
	NAO8.env == noLight -> !NAO8.Vision8;
}
