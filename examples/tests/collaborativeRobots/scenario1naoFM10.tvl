// The feature model 
root NAO10 {
	group [*..*] {
		Core10 group allOf {
			Behaviour10,
			Connection10
		},
		opt Motion10,
		opt Audio10,
		opt Vision10,
		Sensors10,
		opt Trackers10 
	}
	
	//Attribute declarations of the root feature
	enum env in {light , noLight }; 
	
	int speed;
	speed in [30..30];
	
	// Constraints
	Motion10 -> Sensors10;
	NAO10.env == noLight -> !NAO10.Vision10;
}
