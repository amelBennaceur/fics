// The feature model 
root NAO6 {
	group [*..*] {
		Core6 group allOf {
			Behaviour6,
			Connection6
		},
		opt Motion6,
		opt Audio6,
		opt Vision6,
		Sensors6,
		opt Trackers6 
	}
	
	//Attribute declarations of the root feature
	enum env in {light , noLight }; 
	
	int speed;
	speed in [30..30];
	
	// Constraints
	Motion6 -> Sensors6;
	NAO6.env == noLight -> !NAO6.Vision6;
}
