// The feature model 
root NAO4 {
	group [*..*] {
		Core4 group allOf {
			Behaviour4,
			Connection4
		},
		opt Motion4,
		opt Audio4,
		opt Vision4,
		Sensors4,
		opt Trackers4 
	}
	
	//Attribute declarations of the root feature
	enum env in {light , noLight }; 
	
	int speed;
	speed in [30..30];
	
	// Constraints
	Motion4 -> Sensors4;
	NAO4.env == noLight -> !NAO4.Vision4;
}
